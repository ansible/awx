    "jshint": "^2.9.4",
    "jshint-loader": "^0.8.3",
    "jshint-stylish": "^2.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",

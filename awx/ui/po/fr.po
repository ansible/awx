"            <blockquote>\n"
"            {\n"
"                <br/>\"somevar\": \"somevalue\",\n"
"                <br/>\"password\": \"magic\"\n"
"                <br/>\n"
"            }\n"
"            </blockquote>\n"
"                YAML:\n"
"            <br/>\n"
"            <blockquote>\n"
"                ---\n"
"                <br/>somevar: somevalue\n"
"                <br/>password: magic\n"
"                <br/>\n"
"            </blockquote>\n"
"            <p>\n"
